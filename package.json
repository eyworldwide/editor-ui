    "@galacean/engine-toolkit": "^1.3.2",
    "@rollup/plugin-typescript": "^11.1.6",
    "@vitejs/plugin-react": "^4.3.4",
    "chromatic": "^11.7.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.1",
    "rollup-plugin-terser": "^7.0.2",
    "tsup": "^8.2.4",
    "typescript": "^5.7.0",
    "vite": "^6.0.6"
