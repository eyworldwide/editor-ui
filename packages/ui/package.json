{
  "name": "@galacean/editor-ui",
  "version": "0.0.6-beta.1",
  "description": "An opinionated UI library designed to create engaging and functional online tools with ease.",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./recipes": "./design-system/recipes.ts"
  },
  "typesVersions": {
    "*": {
      "recipes": [
        "./design-system/recipes.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "lib/src/index.js",
    "module": "es/src/index.js",
    "types": "types/src/index.d.ts",
    "registry": "https://registry.npmjs.org",
    "exports": {
      ".": {
        "import": "./es/src/index.js",
        "require": "./lib/src/index.js"
      },
      "./recipes": {
        "import": "./es/design-system/recipes.js",
        "require": "./lib/design-system/recipes.js"
      }
    },
    "typesVersions": {
      "*": {
        "recipes": [
          "./typs/design-system/recipes.ts"
        ]
      }
    }
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsup",
    "build:types": "npm run clean:dts && tsc --emitDeclarationOnly --declaration --skipLibCheck --outDir types",
    "build:all": "npm run build && npm run build:types",
    "prepublishOnly": "npm run build:all",
    "clean": "rimraf dist types",
    "clean:dts": "rimraf types",
    "build:nodts": "tsup --no-dts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=CHROMATIC_TOKEN"
  },
  "keywords": [
    "ui",
    "editor",
    "tool",
    "react",
    "3d",
    "inspector"
  ],
  "author": {
    "name": "Mrkou47"
  },
  "files": [
    "dist",
    "types",
    "es",
    "lib"
  ],
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@csstools/postcss-cascade-layers": "^5.0.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "postcss": "^8.4.41",
    "prop-types": "^15.8.1",
    "storybook": "^8.2.9"
  },
  "packageManager": "pnpm@8.6.4",
  "dependencies": {
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.2",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@radix-ui/react-use-controllable-state": "^1.1.0",
    "@radix-ui/react-visually-hidden": "^1.1.1",
    "@stitches/react": "^1.2.8",
    "@tabler/icons-react": "^3.12.0",
    "colord": "^2.9.3",
    "mathjs": "^13.1.1",
    "react-colorful": "^5.6.1",
    "react-hot-toast": "^2.4.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
