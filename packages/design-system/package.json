{
  "name": "@galacean/design-system",
  "version": "1.0.0",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./recipes": {
      "types": "./src/recipes.ts",
      "import": "./src/recipes.ts"
    },
    "./keyframes": {
      "types": "./src/keyframes.ts",
      "import": "./src/keyframes.ts"
    }
  },
  "dependencies": {
    "@radix-ui/colors": "^3.0.0",
    "@stitches/react": "^1.2.8"
  },
  "scripts": {
    "clean": "rimraf es dist",
    "dev:types": "tsup --dts-only",
    "build": "pnpm clean && tsup && pnpm build:types"
  },
  "publishConfig": {
    "access": "public",
    "main": "cjs/index.js",
    "module": "lib/es/index.js",
    "browser": "lib/browser.js",
    "types": "lib/index.d.ts",
    "registry": "https://registry.npmjs.org",
    "exports": {
      ".": {
        "import": "./lib/es/index.js",
        "require": "./lib/cjs/index.js"
      },
      "./parser": {
        "import": "./lib/es/parser.js",
        "require": "./lib/cjs/parser.js"
      }
    },
    "typesVersions": {
      "*": {
        "parser": [
          "./lib/parser.d.ts"
        ]
      }
    }
  }
}